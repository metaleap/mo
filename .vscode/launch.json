{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run",
            "type": "gdb",
            "request": "launch",
            "target": "mo3d.exec",
            "env": {
                "__NV_PRIME_RENDER_OFFLOAD": "1",
                "__VK_LAYER_NV_optimus": "NVIDIA_only",
                "__GLX_VENDOR_LIBRARY_NAME": "nvidia"
            },
            "cwd": "${fileWorkspaceFolder}/bin",
            "valuesFormatting": "parseText",
            "preLaunchTask": "make_devlocal"
        }
    ]
}
